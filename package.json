{
  "name": "manga-api",
  "module": "index.ts",
  "type": "module",
  "private": true,
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@types/bun": "latest",
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.1",
    "globals": "^16.2.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.1",
    "prettier": "^3.5.3",
    "supertest": "^7.1.1",
    "vitest": "^3.2.3"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "scripts": {
    "prepare": "husky",
    "lint": "eslint && prettier . --write",
    "lint:fix": "eslint --fix",
    "dev": "bun --watch index.js",
    "dev:pm2": "pm2 start --interpreter ~/.bun/bin/bun index.js",
    "dev:stop": "pm2 stop all",
    "start": "bun run index.js",
    "test": "vitest"
  },
  "dependencies": {
    "axios": "^1.10.0",
    "cheerio": "^1.1.0",
    "hono": "^4.7.11"
  }
}
